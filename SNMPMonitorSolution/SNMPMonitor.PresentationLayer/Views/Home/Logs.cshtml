
@{
    ViewBag.Title = "Logs";
}

<h2>Logs</h2>
@section Scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            $.ajax({
                url: "@Url.Action("Monitor_Agent_Small", "MonitorTemplate")",
                method: "GET",
                success: function (data) {
                    $("#dataContainer").html(data);
                }
            });
        });
    </script>
}

<div id="dataContainer">

</div>